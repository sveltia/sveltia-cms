{
  "name": "@sveltia/cms",
  "version": "0.119.2",
  "description": "Sveltia CMS is a modern, lightweight, Git-based headless content management system.",
  "keywords": [
    "cms",
    "content-management-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sveltia/sveltia-cms"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:eslint": "eslint .",
    "check:imports": "node scripts/find-unused-imports.js",
    "check:oxlint": "oxlint .",
    "check:prettier": "prettier --check .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "check:svelte": "svelte-check",
    "dev": "vite",
    "format": "prettier --write .",
    "prepublishOnly": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@jsquash/webp": "^1.5.0",
    "@lexical/markdown": "^0.38.2",
    "@sindresorhus/transliterate": "^2.2.0",
    "@sveltia/ui": "^0.31.6",
    "@sveltia/utils": "^0.8.6",
    "create-react-class": "^15.7.0",
    "dayjs": "^1.11.19",
    "deepmerge": "^4.3.1",
    "exifr": "^7.1.3",
    "fast-deep-equal": "^3.1.3",
    "flat": "^6.0.1",
    "isomorphic-dompurify": "^2.32.0",
    "leaflet": "^1.9.4",
    "lexical": "^0.38.2",
    "marked": "^17.0.1",
    "marked-bidi": "^1.0.14",
    "mime": "^4.1.0",
    "parse-entities": "^4.0.2",
    "pdfjs-dist": "^5.4.394",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sanitize-filename": "^1.6.3",
    "smol-toml": "^1.5.2",
    "svelte-i18n": "^4.0.1",
    "svgo": "^4.0.0",
    "terra-draw": "^1.19.0",
    "terra-draw-leaflet-adapter": "1.0.3",
    "turndown": "^7.2.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.6",
    "@vitest/coverage-v8": "^4.0.13",
    "cspell": "^9.3.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.4.0",
    "eslint-plugin-package-json": "^0.85.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsonc-eslint-parser": "^2.4.1",
    "oxlint": "^1.29.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.94.2",
    "stylelint": "^16.26.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-scss": "^6.12.1",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "svelte-preprocess": "^6.0.3",
    "ts-json-schema-generator": "^2.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  },
  "publishConfig": {
    "directory": "package",
    "access": "public",
    "provenance": true
  }
}
